pipeline {
  agent any
  stages {
    stage('Test') {
      steps {
        echo 'Démarrage Test'
        sleep 30
        echo 'Test terminé'
      }
    }

    stage('Build') {
      steps {
        echo 'Démarrage Build'
        sleep 30
        echo 'Build terminé'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Démarrage Deploy'
        sleep 30
        echo 'Deploy terminé'
      }
    }

  }
}